{
  "name": "project-boilerplate",
  "version": "1.0.0",
  "description": "This is my boiler plate for node js personal projects that can be cloned and used as a starting point with all the typical basics in place, especially the packages that are used quite commonly.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./public/styles.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "mongodb": "^6.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0"
  }
}
